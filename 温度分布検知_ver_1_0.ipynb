      },
      "source": [
        "# 実行ボタンがクルクル回っている間にファイルをアップロードする\n",
        "\n",
        "IMAGE_HEIGHT = 4\n",
        "IMAGE_WIDTH = 4\n",
        "\n",
        "def color_red_rate(val):\n",
        "  abs_val = abs(val) # 絶対値 Absolute\n",
        "  color = 'red' if abs_val >= 0.1 else 'black'\n",
        "  return 'text-align: right; width: 60px; background-color: #f0f0f0; color: %s' % color\n",
        "\n",
        "def color_red_diff(val):\n",
        "  abs_val = abs(val) # 絶対値 Absolute\n",
        "  color = 'red' if abs_val >= 8 else 'black'\n",
        "  return 'text-align: right; width: 60px; background-color: #f0f0f0; color: %s' % color\n",
        "  \n",
        "from google.colab import files\n",
        "import pandas as pd\n",
        "import image_segmentation\n",
        "import flir_image_extractor_custom\n",
        "img_seg = image_segmentation.ImageSegmentation()\n",
        "fir = flir_image_extractor_custom.FlirImageExtractor()\n",
        "uploaded = files.upload()\n",
        "temp_list = []\n",
        "for f_nm_ext in list(uploaded.keys()):\n",
        "  # 数値計算\n",
        "  csv_file_name = fir.excute_analysis(f_nm_ext)\n",
        "  # 画像分割や平均算出\n",
        "  temp_list.append( img_seg.excute_analysis(f_nm_ext, csv_file_name, IMAGE_HEIGHT, IMAGE_WIDTH) )\n",
        "\n",
        "# 温度比較 temp_list\n",
        "temp_list = img_seg.diff_temperature(temp_list)\n",
        "# 通常の表示用\n",
        "index1 = [\"基本\", \"比較\", \"diff\", \"rate\"]\n",
        "list1=temp_list\n",
        "df = pd.DataFrame(data=list1, index=index1)\n",
        "df.columns = df.columns + 1\n",
        "df"
      ],
      "execution_count": 0,
      "outputs": []
